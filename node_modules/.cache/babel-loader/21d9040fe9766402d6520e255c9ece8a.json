n                                    <img src=\"../../public/images/ghana-flag.png\"\n                                    alt=\"trade bitcoin in ghana\">\n                                      {{ rate.buying_price }} gh |\n                                      <img src=\"../../public/images/usd.png\"\n                                       alt=\"\"> {{ rate.buying_price * 10}} Usd\n                                        <strong   @click=\"displayBuy(rate.id)\">buy<i class=\"fas fa-store\"\n                                        ></i></strong>\n                                  </div>\n\n                                </div>\n\n                </div>\n\n                <div class=\"box\" v-if=\"showItems\">\n                  <div class=\"\" v-for=\"item in items\" :key=\"item.id\">\n                    numero  {{ item.id }}\n                  </div>\n                </div>\n\n        </div>\n\n        <Buy v-bind:id=\"item_id\" v-if=\"showBuy\"></Buy>\n    </div>\n</template>\n\n\n<script>\n    import Buy from '@/components/Buy'\n\n    export default {\n        name: 'MainUser',\n      data(){\n        return{\n            details:[],\n            rates: [],\n            items: [],\n            showItems: false,\n            showInfos: true,\n            infos: [],\n            id: '',\n            showBuy: false\n        }\n      },\n      components: {\n        Buy,\n      },\n      mounted: function(){\n          this.displayInfos();\n      },\n      methods:{\n      displayInfos(){\n        axios.get('http://127.0.0.1/rapidnote/api/rates').then(\n                    response =>\n                    this.rates = response.data);\n                  /*  axios.get('http://127.0.0.1/rapidnote/api/userById/1').then(\n                    response =>\n                    this.details = response.data);*/\n        this.showInfos = true;\n      },\n      getImgUrl(pic) {\n                return \"http://127.0.0.1/rapidnote/public/images/\" + pic;\n      },\n      displayBuy(id){\n        this.showBuy = true;\n        this.showInfos = false;\n      }\n    }\n\n    }\n    </script>\n\n    <!-- Add \"scoped\" attribute to limit CSS to this component only -->\n    <style scoped>\n\n    </style>"]},"metadata":{},"sourceType":"module"}