            <div class=\"box\" v-for=\"rate in rates\" :key=\"rate.id\">\n                               <div class=\"devise\"> <img :src=\"getImgUrl(rate.image)\" >\n                                {{ rate.name }}\n\n                               </div>\n                                <div class=\"note\">\n                                  <h3>\n                                    Selling\n                                  </h3>\n\n                                  <div class=\"important\">\n                                    {{ rate.selling_price }} gh <img src=\"../../public/images/ghana-flag.png\" alt=\"trade bitcoin in ghana\"> ! {{ rate.selling_price * 10}} Usd <img src=\"../../public/images/usd.png\" alt=\"\"> <strong>Buy <i class=\"fas fa-shopping-cart\"></i></strong>\n                                  </div>\n\n                                  <h3>\n                                    Buying\n                                  </h3>\n\n                                  <div class=\"important\">\n                                    {{ rate.buying_price }} gh <img src=\"../../public/images/ghana-flag.png\" alt=\"trade bitcoin in ghana\"> ! {{ rate.buying_price * 10}} Usd <img src=\"../../public/images/usd.png\" alt=\"\"> <strong>Buy <i class=\"fas fa-shopping-cart\"></i></strong>\n                                  </div>\n\n                                </div>\n\n                </div>\n        </div>\n    </div>\n</template>\n\n\n<script>\n    export default {\n        name: 'MainUser',\n      data(){\n        return{\n            details:[],\n            rates: [],\n            showInfos: true\n        }\n      },\n      mounted: function(){\n          this.displayInfos();\n      },\n      methods:{\n      displayInfos(){\n        axios.get('http://127.0.0.1/rapidnote/api/rates').then(\n                    response =>\n                    this.rates = response.data);\n                  /*  axios.get('http://127.0.0.1/rapidnote/api/userById/1').then(\n                    response =>\n                    this.details = response.data);*/\n        this.showInfos = true;\n      },\n      getImgUrl(pic) {\n                return \"http://127.0.0.1/rapidnote/public/images/\" + pic;\n            }\n    }\n\n    }\n    </script>\n\n    <!-- Add \"scoped\" attribute to limit CSS to this component only -->\n    <style scoped>\n\n    </style>"]},"metadata":{},"sourceType":"module"}