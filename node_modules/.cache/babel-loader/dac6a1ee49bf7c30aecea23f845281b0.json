{"ast":null,"code":"export default {\n  name: 'MainAdmin',\n\n  data() {\n    return {\n      details: [],\n      showInfos: true,\n      showEditRate: false,\n      nbr: ''\n    };\n  },\n\n  mounted: function () {\n    this.getRates();\n  },\n  methods: {\n    getRates() {\n      axios.get('http://127.0.0.1/rapidnote/api/totalTransactionsValue').then(response => this.nbr = response.data);\n      axios.get('http://127.0.0.1/rapidnote/api/rates').then(response => this.details = response.data), this.showEditRate = false;\n      this.showInfos = true;\n    },\n\n    displayEditRate(detail_id) {\n      axios.get('http://127.0.0.1/rapidnote/api/rateById/' + detail_id).then(response => this.details = response.data);\n      this.showEditRate = true;\n      this.showInfos = false;\n    },\n\n    displayInfos() {\n      this.showEditRate = false;\n      ;\n      alert('operation enregistree');\n      this.showInfos = true;\n    },\n\n    getBuyprice(buying_price) {\n      return buying_price;\n    },\n\n    getImgUrl(pic) {\n      return \"http://127.0.0.1/rapidnote/public/images/\" + pic;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuEI,eAAe;EACXA,IAAI,EAAE,WADK;;EAEbC,IAAI,GAAE;IACJ,OAAM;MACFC,OAAO,EAAC,EADN;MAEFC,SAAS,EAAE,IAFT;MAGFC,YAAY,EAAE,KAHZ;MAIFC,GAAG,EAAE;IAJH,CAAN;EAMD,CATY;;EAUbC,OAAO,EAAE,YAAU;IACf,KAAKC,QAAL;EACH,CAZY;EAabC,OAAO,EAAC;IACND,QAAQ,GAAG;MACHE,KAAK,CAACC,GAAN,CAAU,uDAAV,EAAmEC,IAAnE,CACIC,QAAO,IACP,KAAKP,GAAL,GAAWO,QAAQ,CAACX,IAFxB;MAGHQ,KAAK,CAACC,GAAN,CAAU,sCAAV,EAAkDC,IAAlD,CACOC,QAAO,IACP,KAAKV,OAAL,GAAeU,QAAQ,CAACX,IAF/B,GAKD,KAAKG,YAAL,GAAoB,KALnB;MAMD,KAAKD,SAAL,GAAiB,IAAjB;IACH,CAZK;;IAcRU,eAAe,CAACC,SAAD,EAAW;MACxBL,KAAK,CAACC,GAAN,CAAU,6CAA6CI,SAAvD,EAAkEH,IAAlE,CACYC,QAAO,IACP,KAAKV,OAAL,GAAeU,QAAQ,CAACX,IAFpC;MAIA,KAAKG,YAAL,GAAoB,IAApB;MACA,KAAKD,SAAL,GAAiB,KAAjB;IACD,CArBO;;IAsBRY,YAAY,GAAE;MACZ,KAAKX,YAAL,GAAoB,KAApB;MAA0B;MAC1BY,KAAK,CAAC,uBAAD,CAAL;MACA,KAAKb,SAAL,GAAiB,IAAjB;IACD,CA1BO;;IA2BRc,WAAW,CAACC,YAAD,EAAe;MAChB,O