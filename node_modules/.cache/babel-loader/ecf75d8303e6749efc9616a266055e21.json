{"ast":null,"code":"export default {\n  name: 'MainAdmin',\n\n  data() {\n    return {\n      details: [],\n      showInfos: true,\n      showEditBtc: false,\n      showEditPM: false,\n      nbr: ''\n    };\n  },\n\n  mounted: function () {\n    this.getTotalTransactionsValue(); // this.getDetails();\n  },\n  methods: {\n    getTotalTransactionsValue() {\n      axios.get('http://127.0.0.1/rapidnote/api/totalTransactionsValue').then(response => this.nbr = response.data);\n    },\n\n    getRates() {\n      axios.get('http://127.0.0.1/rapidnote/api/rates').then(response => this.details = response.data);\n    },\n\n    displayEditBtc() {\n      this.showEditBtc = true;\n      this.showEditPM = false;\n      this.showInfos = false;\n    },\n\n    displayEditPM() {\n      this.showEditPM = true;\n      this.showEditBtc = false;\n      this.showInfos = false;\n    },\n\n    displayInfos() {\n      this.showEditPM = false;\n      this.showEditBtc = false;\n      alert('operation enregistree');\n      this.showInfos = true;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0CI,eAAe;EACXA,IAAI,EAAE,WADK;;EAEbC,IAAI,GAAE;IACJ,OAAM;MACFC,OAAO,EAAC,EADN;MAEFC,SAAS,EAAE,IAFT;MAGFC,WAAW,EAAE,KAHX;MAIFC,UAAU,EAAE,KAJV;MAKFC,GAAG,EAAE;IALH,CAAN;EAOD,CAVY;;EAWbC,OAAO,EAAE,YAAU;IACf,KAAKC,yBAAL,GADe,CAEhB;EACF,CAdY;EAebC,OAAO,EAAC;IACND,yBAAyB,GAAG;MACpBE,KAAK,CAACC,GAAN,CAAU,uDAAV,EAAmEC,IAAnE,CACIC,QAAO,IACP,KAAKP,GAAL,GAAWO,QAAQ,CAACZ,IAFxB;IAGH,CALC;;IAMFa,QAAQ,GAAG;MACPJ,KAAK,CAACC,GAAN,CAAU,sCAAV,EAAkDC,IAAlD,CACIC,QAAO,IACP,KAAKX,OAAL,GAAeW,QAAQ,CAACZ,IAF5B;IAGH,CAVC;;IAYRc,cAAc,GAAE;MACd,KAAKX,WAAL,GAAmB,IAAnB;MACA,KAAKC,UAAL,GAAkB,KAAlB;MACA,KAAKF,SAAL,GAAiB,KAAjB;IACD,CAhBO;;IAiBRa,aAAa,GAAE;MACb,KAAKX,UAAL,GAAkB,IAAlB;MACA,KAAKD,WAAL,GAAmB,KAAnB;MACA,KAAKD,SAAL,GAAiB,KAAjB;IACD,CArBO;;IAsBRc,YAAY,GAAE;MACZ,KAAKZ,UAAL,GAAkB,KAAlB;MACA,KAAKD,WAAL,GAAmB,KAAnB;MACAc,KAAK,CAAC,uBAAD,CAAL;MACA,KAAKf,SAAL,GAAiB,IAAjB;IACD;;EA3BO;AAfK,CAAf","names":["name","data","details","showI